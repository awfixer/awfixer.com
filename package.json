{
  "name": "awfixer-website",
  "version": "1.0.0",
  "description": "AWFixer and Friends website",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "ci": "rm -rf .next node_modules pnpm-lock.yaml && pnpm i && payload migrate && pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "eslint . --fix",
    "lintold": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lintold:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "typecheck": "tsc --noEmit",
    "validate:styles": "bash scripts/validate-styles.sh",
    "validate:all": "pnpm run typecheck && pnpm run lint && pnpm run validate:styles",
    "prepare": "husky"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.60.0",
    "@payloadcms/db-vercel-postgres": "3.60.0",
    "@payloadcms/live-preview-react": "3.60.0",
    "@payloadcms/next": "3.60.0",
    "@payloadcms/payload-cloud": "3.60.0",
    "@payloadcms/plugin-form-builder": "3.60.0",
    "@payloadcms/plugin-multi-tenant": "3.60.0",
    "@payloadcms/plugin-nested-docs": "3.60.0",
    "@payloadcms/plugin-redirects": "3.60.0",
    "@payloadcms/plugin-search": "3.60.0",
    "@payloadcms/plugin-sentry": "3.60.0",
    "@payloadcms/plugin-seo": "3.60.0",
    "@payloadcms/plugin-stripe": "3.60.0",
    "@payloadcms/richtext-lexical": "3.60.0",
    "@payloadcms/storage-vercel-blob": "3.60.0",
    "@payloadcms/ui": "3.60.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.6.1",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "15.4.7",
    "next-sitemap": "^4.2.3",
    "payload": "3.60.0",
    "payload-totp": "3.0.0-beta.4",
    "prism-react-renderer": "^2.3.1",
    "react": "19.1.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.34.4",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vercel": "^48.4.0"
  },
  "devDependencies": {
    "@eslint/css": "^0.13.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.38.0",
    "@eslint/json": "^0.13.2",
    "@playwright/test": "1.54.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "26.1.0",
    "lint-staged": "^16.2.4",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.46.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sentry/cli",
      "bufferutil",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "utf-8-validate"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
