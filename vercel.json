{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "fumadocs-mdx && next build",
  "cleanUrls": true,
  "devCommand": "next dev",
  "redirects": [
    {
      "source": "/help",
      "destination": "/help/docs",
      "permanent": true
    },
    {
      "source": "/help/",
      "destination": "/help/docs",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/help/docs/:path*",
      "destination": "/help/docs/:path*"
    }
  ],
  "headers": [
    {
      "source": "/help/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/blog",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-src https://awfixer.blog https://cdn.jsdelivr.net; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net; style-src 'self' 'unsafe-inline'"
        }
      ]
    }
  ]
}
